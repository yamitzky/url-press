{
  "name": "url-press",
  "version": "1.0.0",
  "description": "URL shortener for teams",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "build/server/index.js",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "pnpx @biomejs/biome check --write app",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "prepublishOnly": "remix vite:build"
  },
  "keywords": ["url-shortener", "team", "dynamodb"],
  "author": "Mitsuki Ogasahara",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamitzky/url-press.git"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.616.0",
    "@nextui-org/react": "^2.4.2",
    "@remix-run/node": "^2.10.3",
    "@remix-run/react": "^2.10.3",
    "@remix-run/serve": "^2.10.3",
    "@rvf/remix": "^0.0.10",
    "@rvf/zod": "^0.0.10",
    "framer-motion": "^11.3.6",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.2",
    "@remix-run/dev": "^2.10.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
